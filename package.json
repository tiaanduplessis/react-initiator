{
  "name": "react-initiator",
  "version": "1.0.0",
  "description": "Another opinionated React project seed",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot --history-api-fallback",
    "dashboard": "webpack-dashboard -- yarn start",
    "coverage": "jest --coverage",
    "build": "NODE_ENV=production webpack --mode production --progress",
    "serve": "serve dist",
    "test": "jest",
    "flow": "flow",
    "lint": "eslint --fix ./**/*.js",
    "prepush": "yarn lint; yarn flow; yarn test"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-flow": "7.0.0-beta.46",
    "@babel/preset-react": "7.0.0-beta.46",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jest": "^21.15.2",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "1.1.11",
    "flow-bin": "0.72.0",
    "glob-all": "3.1.0",
    "hard-source-webpack-plugin": "0.6.7",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "mini-css-extract-plugin": "0.4.0",
    "optimize-css-assets-webpack-plugin": "4.0.1",
    "postcss-cssnext": "3.1.0",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.5",
    "purify-css": "1.2.5",
    "purifycss-webpack": "0.7.0",
    "serve": "6.5.6",
    "style-loader": "0.21.0",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-order": "0.8.1",
    "stylelint-webpack-plugin": "0.10.4",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3",
    "webpack-dashboard": "1.1.1",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "react": "16.3.2",
    "react-dom": "16.3.2"
  },
  "browserslist": [
    "> 1%"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/css-transform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/file-transform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
